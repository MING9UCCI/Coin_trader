# 업비트(Upbit) API 키를 발급받아 이곳에 입력하고 파일명을 .env 로 변경하세요.

UPBIT_ACCESS_KEY=your_upbit_access_key
UPBIT_SECRET_KEY=your_upbit_secret_key

# 매매 설정
SYMBOL=KRW-BTC          # 거래할 코인 (원화 마켓 - 비트코인)
TRADE_AMOUNT=10000      # 1회 매수 시도 금액 (원 단위). 예: 1만원
DRY_RUN=True            # True이면 실제 매매를 하지 않고 시뮬레이션만 합니다. (테스트용)
